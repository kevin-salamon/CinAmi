{
  "name": "interview-app",
  "version": "1.0.0",
  "description": "interview app",
  "main": "server.js",
  "engines": {
    "node": "10.16.3"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node seeds/seed.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevin-salamon/interview-app.git"
  },
  "author": "Kevin Salamon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kevin-salamon/interview-app/issues"
  },
  "homepage": "https://github.com/kevin-salamon/interview-app#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.17",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
